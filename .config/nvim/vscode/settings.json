{
    "vscode-neovim.neovimExecutablePaths.linux": "/usr/local/bin/nvim",
    "vscode-neovim.neovimInitVimPaths.linux": "$HOME/.config/nvim/init.vim",
    "workbench.list.automaticKeyboardNavigation": false,
    "code-runner.runInTerminal": true,
    "editor.lineNumbers": "relative",
    "workbench.colorTheme": "Nord Deep",
    "workbench.colorCustomizations": {
        "editorBracketHighlight.foreground1": "#b48ead",
        "editorBracketHighlight.foreground2": "#ebcb8b",
        "editorBracketHighlight.foreground3": "#8fbcbb"
    },
    "editor.bracketPairColorization.enabled": true,
    "editor.fontFamily": "'FiraCode Nerd Font'",
    "editor.fontLigatures": true
}
